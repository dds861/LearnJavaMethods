        android:layout_below="@+id/et_param4"
        android:layout_toLeftOf="@id/iv_delete_param1"
        android:src="@drawable/ic_action_quotes" />

    <ImageView
        android:id="@+id/iv_delete_param5"
        style="@style/iv_style_params_delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/et_param4"
        android:layout_alignParentEnd="true"
        android:src="@android:drawable/ic_menu_close_clear_cancel" />

    <EditText
        android:id="@+id/et_param6"
        style="@style/et_style_params"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/et_param5"
        android:layout_toLeftOf="@id/iv_quotes_param1"
        android:hint="param6" />


    <ImageView
        android:id="@+id/iv_quotes_param6"
        style="@style/iv_style_params_quotes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/et_param5"
        android:layout_toLeftOf="@id/iv_delete_param1"
        android:src="@drawable/ic_action_quotes" />

    <ImageView
        android:id="@+id/iv_delete_param6"
        style="@style/iv_style_params_delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/et_param5"
        android:layout_alignParentEnd="true"
        android:src="@android:drawable/ic_menu_close_clear_cancel" />

    <EditText
        android:id="@+id/et_param7"
        style="@style/et_style_params"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/et_param6"
        android:layout_toLeftOf="@id/iv_quotes_param1"
        android:hint="param7" />


    <ImageView
        android:id="@+id/iv_quotes_param7"
