        android:layout_height="wrap_content"
        android:layout_below="@+id/et_param6"
        android:layout_toLeftOf="@id/iv_delete_param1"
        android:src="@drawable/ic_action_quotes" />

    <ImageView
        android:id="@+id/iv_delete_param7"
