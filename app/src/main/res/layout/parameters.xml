        android:layout_height="wrap_content"
        android:layout_below="@+id/et_param6"
        android:layout_alignParentEnd="true"
        android:src="@android:drawable/ic_menu_close_clear_cancel" />

    <EditText
        android:id="@+id/et_param8"
